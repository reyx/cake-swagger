{
  "name": "aymard-pro/cake-swagger",
  "description": "Swagger plugin for CakePHP v3",
  "type": "cakephp-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "AYMARD KOUAKOU",
      "email": "aymard.pro@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "cakephp/cakephp": "3.5.*",
    "zircote/swagger-php": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=6.1 <7.0.0"
  },
  "autoload": {
    "psr-4": {
      "CakeSwagger\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CakeSwagger\\Tests\\": "tests"
    }
  }
}
